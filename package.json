{
  "name": "mockdesk",
  "description": "Desktop application for creating mockups",
  "version": "0.6.0",
  "homepage": "https://github.com/twolfson/mockdesk",
  "author": {
    "name": "Todd Wolfson",
    "email": "todd@twolfson.com",
    "url": "http://twolfson.com/"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/twolfson/mockdesk.git"
  },
  "bugs": {
    "url": "https://github.com/twolfson/mockdesk/issues"
  },
  "bin": {
    "mockdesk": "bin/mockdesk.js"
  },
  "main": "lib/mockdesk",
  "engines": {
    "node": ">= 0.10.0"
  },
  "license": "Proprietary",
  "scripts": {
    "develop": "gulp develop",
    "develop-test": "karma start",
    "debug-repl": "rc /tmp/repl/mockdesk.sock",
    "lint": "twolfson-style lint lib/ test/",
    "postinstall": "cd node_modules && git clone https://github.com/twolfson/karma && cd karma && git checkout 03273f2 && npm install && npm run build && cd .. && cd ..",
    "postinstall-comment": "We want to use `git` dependencies but for some reason, they don't work for `karma`",
    "start": "node bin/mockdesk.js",
    "start-develop": "node bin/mockdesk.js --livereload",
    "test": "npm run test-karma && npm run lint",
    "test-karma": "karma start --single-run",
    "test-windows": "npm run test-karma"
  },
  "dependencies": {
    "commander": "~2.9.0",
    "domo": "~0.5.9",
    "electron-prebuilt": "~0.36.8",
    "replify": "~1.2.0",
    "underscore": "~1.8.3",
    "winston": "~2.1.1"
  },
  "devDependencies": {
    "chai": "~3.5.0",
    "foundry": "~4.0.3",
    "foundry-release-git": "~2.0.2",
    "foundry-release-npm": "~2.0.2",
    "gulp": "~3.9.1",
    "gulp-livereload": "~3.8.1",
    "jscs": "~1.7.3",
    "jshint": "~2.5.10",
    "karma-electron": "git://github.com/twolfson/karma-electron#acd9e05",
    "karma-mocha": "~0.2.2",
    "mocha": "~1.11.0",
    "repl-client": "~0.3.0",
    "twolfson-style": "~1.6.0"
  },
  "keywords": [
    "mock",
    "desktop",
    "mockup"
  ],
  "private": true,
  "foundry": {
    "releaseCommands": [
      "foundry-release-git",
      "foundry-release-npm"
    ]
  }
}
